{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "localstart": "npx nodemon localIndex.js",
    "gen_docs": "npx jsdoc2md -f ./src/* ./src/datasources/* localIndex.js > ../docs/graphql.md",
    "serve_docs": "npx docsify serve ../docs",
    "firestore": "firebase emulators:start --only firestore",
    "test": "firebase emulators:exec \"jest --watch\" "
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.7.0",
    "apollo-datasource": "^0.7.2",
    "apollo-server": "^2.19.0",
    "apollo-server-express": "^2.19.0",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^8.2.0",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.0",
    "geofirestore": "^4.4.1",
    "google-spreadsheet": "^2.0.6",
    "graphql": "^15.4.0",
    "graphql-resolvers": "^0.4.2",
    "graphql-tools": "^7.0.2",
    "graphql-voyager": "^1.0.0-rc.29",
    "line-reader": "^0.4.0",
    "linebot": "^1.6.0",
    "lodash": "^4.17.20",
    "querystring": "^0.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "request-promise": "^4.2.6",
    "sharp": "^0.26.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.11",
    "@types/jest": "^26.0.19",
    "apollo-server-testing": "^2.19.0",
    "cz-conventional-changelog": "^3.3.0",
    "docsify-cli": "^4.4.2",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "graphql-tag": "^2.11.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jsdoc-to-markdown": "^6.0.1",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
